{
  "name": "web-scan",
  "type": "module",
  "version": "2.0.0",
  "homepage": "https://web-scan.netlify.app",
  "scripts": {
    "start": "node server",
    "start-pm": "pm2 start server.js -i max",
    "build": "astro check && astro build",
    "dev:vercel": "PLATFORM='vercel' npx vercel dev",
    "dev:netlify": "PLATFORM='netlify' npx netlify dev",
    "dev:api": "DISABLE_GUI='true' PORT='3001' nodemon server",
    "dev:api-sqlite": "DISABLE_GUI='true' PORT='3001' nodemon server-dev",
    "dev:astro": "PUBLIC_API_ENDPOINT=http://localhost:3001/api astro dev",
    "dev": "concurrently -c magenta,cyan -n backend,frontend 'yarn dev:api' 'yarn dev:astro'",
    "dev:sqlite": "concurrently -c magenta,cyan -n backend,frontend 'yarn dev:api-sqlite' 'yarn dev:astro'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:api": "node tests/run-api-tests.js",
    "test:api:benchmarks": "node tests/run-api-tests.js --exit-on-failure",
    "db:migrate": "node src/database/migrate.js",
    "db:seed": "node src/database/seed.js",
    "db:reset": "node src/database/reset.js",
    "lint": "eslint src --ext .ts,.tsx,.js",
    "lint:fix": "eslint src --ext .ts,.tsx,.js --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.10",
    "@astrojs/react": "^3.3.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/svelte-fontawesome": "^0.2.2",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "astro": "^4.16.19",
    "axios": "^1.12.0",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.12",
    "chrome-aws-lambda": "^10.1.0",
    "chromium": "^3.0.3",
    "compromise": "^14.10.0",
    "connect-history-api-fallback": "^2.0.0",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.20.0",
    "express-rate-limit": "^7.2.0",
    "framer-motion": "^11.2.6",
    "got": "^14.2.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "ml-matrix": "^6.10.9",
    "multer": "^2.0.2",
    "natural": "^6.12.0",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.14",
    "pdf-lib": "^1.17.1",
    "pg": "^8.12.0",
    "pm2": "^5.3.1",
    "psl": "^1.9.0",
    "puppeteer": "^22.8.0",
    "puppeteer-core": "^22.8.0",
    "winston": "^3.11.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-masonry-css": "^1.0.16",
    "react-router-dom": "^6.23.0",
    "react-simple-maps": "^3.0.0",
    "react-toastify": "^10.0.5",
    "recharts": "^2.12.6",
    "redis": "^4.7.0",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "svelte": "^4.2.19",
    "typescript": "^5.4.5",
    "unzipper": "^0.11.5",
    "url-parse": "^1.5.10",
    "uuid": "^10.0.0",
    "wappalyzer": "^6.10.65",
    "ws": "^8.18.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@astrojs/cloudflare": "^10.2.5",
    "@astrojs/netlify": "^5.2.0",
    "@astrojs/node": "^9.4.1",
    "@astrojs/partytown": "^2.1.0",
    "@astrojs/sitemap": "^3.1.4",
    "@astrojs/svelte": "^5.4.0",
    "@astrojs/ts-plugin": "^1.6.1",
    "@astrojs/vercel": "^7.5.4",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.12",
