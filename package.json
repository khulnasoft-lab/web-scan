    "@types/pg": "^8.11.6",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.12",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "sass": "^1.77.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5"
  }
